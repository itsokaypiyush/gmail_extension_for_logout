{
  "manifest_version": 3,
  "name": "Gmail Auto Logout",
  "version": "1.0",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["http://127.0.0.1:5001/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
